{{#each projects as |project| }}
<div class="row align-center mb-5">
  <div class="col-md-5">
    <h2>{{{get_emoji}}} {{project.name}}</h2>
  </div>
  <div class="col-md-5">
    <h3>{{project.description}}</h3>
  </div>
  <div class="col-md-2">
    <h3>{{project.needed_funding}}</h3>
  </div>
</div>
{{/each}}
